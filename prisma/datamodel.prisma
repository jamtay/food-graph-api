type User {
  id: ID! @id
  email: String! @unique
  password: String!
  name: String!
  foods: [FoodItem!]!
}

type FoodItem {
  id: ID! @id
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
  name: String!
  description: String
  cost: Float
  calories: Int
  protein: Int
  vegan: Boolean
  createdBy: User!
}